  "paths":{
      "/v5/repos/{owner}/{repo}/branches":{
          "get":{
              "summary":"获取所有分支",
              "description":"获取所有分支",
              "produces":[
                  "application/json"
              ],
              "parameters":[
                  {
                      "in":"query",
                      "name":"access_token",
                      "description":"用户授权码",
                      "type":"string",
                      "default":"6acf6380ce0a8aa993eb235e62aef326",
                      "enum":[
                          "6acf6380ce0a8aa993eb235e62aef326"
                      ],
                      "required":true
                  },
                  {
                      "in":"path",
                      "name":"owner",
                      "description":"仓库所属空间地址(企业、组织或个人的地址path)",
                      "type":"string",
                      "default":"giteeshao",
                      "enum":[
                          "giteeshao"
                      ],
                      "required":true
                  },
                  {
                      "in":"path",
                      "name":"repo",
                      "description":"仓库路径(path)",
                      "type":"string",
                      "default":"cfp",
                      "enum":[
                          "cfp"
                      ],
                      "required":true
                  }
              ],
              "responses":{
                  "200":{
                      "description":"返回格式",
                      "schema":{
                          "type":"array",
                          "items":{
                              "$ref":"#/definitions/Branch"
                          }
                      }
                  }
              },
              "tags":[
                  "Repositories"
              ],
              "operationId":"getV5ReposOwnerRepoBranches"
          },
          "post":{
              "summary":"创建分支",
              "description":"创建分支",
              "produces":[
                  "application/json"
              ],
              "consumes":[
                  "application/json"
              ],
              "parameters":[
                  {
                      "in":"formData",
                      "name":"access_token",
                      "description":"用户授权码",
                      "type":"string",
                      "default":"6acf6380ce0a8aa993eb235e62aef326",
                      "enum":[
                          "6acf6380ce0a8aa993eb235e62aef326"
                      ],
                      "required":true
                  },
                  {
                      "in":"path",
                      "name":"owner",
                      "description":"仓库所属空间地址(企业、组织或个人的地址path)",
                      "type":"string",
                      "default":"giteeshao",
                      "enum":[
                          "giteeshao"
                      ],
                      "required":true
                  },
                  {
                      "in":"path",
                      "name":"repo",
                      "description":"仓库路径(path)",
                      "type":"string",
                      "default":"cfp",
                      "enum":[
                          "cfp"
                      ],
                      "required":true
                  },
                  {
                      "in":"formData",
                      "name":"refs",
                      "description":"起点名称, 默认：master",
                      "type":"string",
                      "default":"master",
                      "required":true
                  },
                  {
                      "in":"formData",
                      "name":"branch_name",
                      "description":"新创建的分支名称",
                      "type":"string",
                      "required":true
                  }
              ],
              "responses":{
                  "201":{
                      "description":"返回格式",
                      "schema":{
                          "$ref":"#/definitions/CompleteBranch"
                      }
                  }
              },
              "tags":[
                  "Repositories"
              ],
              "operationId":"postV5ReposOwnerRepoBranches"
          }
      }
  }
